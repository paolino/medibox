<!DOCTYPE patchbay>
<patchbay version="0.3.10" name="jackp">
 <output-sockets>
  <socket exclusive="off" client="system" type="jack-audio" name="system">
   <plug>capture_1</plug>
   <plug>capture_2</plug>
  </socket>
  <socket exclusive="off" client="SuperCollider" type="jack-audio" name="SuperCollider">
   <plug>out_1</plug>
   <plug>out_2</plug>
   <plug>out_3</plug>
   <plug>out_4</plug>
   <plug>out_5</plug>
   <plug>out_6</plug>
   <plug>out_7</plug>
   <plug>out_8</plug>
  </socket>
  <socket exclusive="off" client="Midi Through" type="alsa-midi" name="Midi Through">
   <plug>Midi Through Port-0</plug>
  </socket>
  <socket exclusive="off" client="BCR[0-9]+" type="alsa-midi" name="BCR2000">
   <plug>BCR[0-9]+ MIDI 1</plug>
   <plug>BCR[0-9]+ MIDI 2</plug>
  </socket>
  <socket exclusive="off" client="player: feedbackctrl out" type="alsa-midi" name="player: feedbackctrl out">
   <plug>midi out</plug>
  </socket>
  <socket exclusive="off" client="showwave: feedbackctrl out" type="alsa-midi" name="showwave: feedbackctrl out">
   <plug>midi out</plug>
  </socket>
  <socket exclusive="off" client="player" type="alsa-midi" name="player">
   <plug>midi out</plug>
  </socket>
 </output-sockets>
 <input-sockets>
  <socket exclusive="off" client="system" type="jack-audio" name="system">
   <plug>playback_1</plug>
   <plug>playback_2</plug>
  </socket>
  <socket exclusive="off" client="SuperCollider" type="jack-audio" name="SuperCollider">
   <plug>in_1</plug>
   <plug>in_2</plug>
   <plug>in_3</plug>
   <plug>in_4</plug>
   <plug>in_5</plug>
   <plug>in_6</plug>
   <plug>in_7</plug>
   <plug>in_8</plug>
  </socket>
  <socket exclusive="off" client="Midi Through" type="alsa-midi" name="Midi Through">
   <plug>Midi Through Port-0</plug>
  </socket>
  <socket exclusive="off" client="BCR[0-9]+" type="alsa-midi" name="BCR2000">
   <plug>BCR[0-9]+ MIDI 1</plug>
   <plug>BCR[0-9]+ MIDI 2</plug>
   <plug>BCR[0-9]+ MIDI 3</plug>
  </socket>
  <socket exclusive="off" client="player: inputctrl in" type="alsa-midi" name="player: inputctrl in">
   <plug>midi in</plug>
  </socket>
  <socket exclusive="off" client="showwave: inputctrl in" type="alsa-midi" name="showwave: inputctrl in">
   <plug>midi in</plug>
  </socket>
 </input-sockets>
 <slots/>
 <cables>
  <cable output="SuperCollider" input="system" type="jack-audio"/>
  <cable output="BCR2000" input="player: inputctrl in" type="alsa-midi"/>
  <cable output="BCR2000" input="showwave: inputctrl in" type="alsa-midi"/>
  <cable output="player: feedbackctrl out" input="BCR2000" type="alsa-midi"/>
  <cable output="player: feedbackctrl out" input="showwave: inputctrl in" type="alsa-midi"/>
  <cable output="showwave: feedbackctrl out" input="BCR2000" type="alsa-midi"/>
 </cables>
</patchbay>

